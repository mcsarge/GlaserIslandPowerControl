    {
        "id": "8e7f112711f45234",
        "type": "function",
        "z": "7f359afa7c82b177",
        "name": "HandleCameraButtonOnOff",
        "func": "if (msg.payload /= 0) {\n    return null;\n}\nelse\n{\n    var values = global.get([\"ssr1\", \"ssr3\"]);\n    var anyOn = false;\n    \n    values.forEach(function(element) {\n        anyOn = ((element===\"on\") || anyOn) \n    });\n    \n    //If any cameras are on, turn them off, else turn them on.\n    if (anyOn)\n    {\n        msg.payload = \"off\";\n    }\n    else\n    {\n        msg.payload = \"on\"\n    }\n}\nreturn msg;\n",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 340,
        "y": 160,
        "wires": [
